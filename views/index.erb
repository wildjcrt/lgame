<!DOCTYPE html>
<html>
  <head>
    <title>L Game</title>
    <meta charset="utf-8">
    <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
    <style type="text/css" media="screen">
      body {
        font-size: 100px;
      }
      #container {
        position: relative;
      }
      .table {
        border-collapse: collapse;
      }
      .table td {
        border: 1px solid #333;
        width: 120px;
        height: 120px;
      }
      #l1, #l2, #o1, #o2 {
        position: absolute;
      }
      #l1, #l2 {
        border-left: 122px solid;
        border-top: 122px solid;
        width: 124px;
        height: 246px;
        cursor: pointer;
      }
      #l1 {
        border-color: blue;
      }
      #l2 {
        border-color: red;
      }
      #o1, #o2 {
        background: #333;
        border-radius: 50%;
        height: 120px;
        width: 120px;
        cursor: pointer;
      }
    </style>
  </head>
  <body>
    <div id="container">
      <table class="table">
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
      </table>

      <div id="l1"></div>
      <div id="l2"></div>
      <div id="o1"></div>
      <div id="o2"></div>
    </div>
  </body>
</html>
